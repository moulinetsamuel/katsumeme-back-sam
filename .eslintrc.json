{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions" :"off",
        "no-underscore-dangle" : "off",
        "max-len": "off",
        "max-classes-per-file": "off",
        "no-param-reassign": "off"
    }
}
